//Single level Inheritance
import java.util.Scanner;
class A {
	int a = 12;
	static int b = 13;
	public void fun() {
		System.out.println("Hello");
	}

}
class B extends A{
	int c = 14;
	static int d = 15;
	public void fun1() {
		System.out.println("Hi");
	}
}
public class Gi1{
	public static void main(String[] args) {
		B obj = new B();
		System.out.println(obj.a+" "+obj.c);
		obj.fun();
		obj.fun1();
		System.out.println(B.d);
		System.out.println(B.b);
	}
}
