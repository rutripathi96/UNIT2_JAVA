//Instance Intializer Block

class Animal{
	public Animal(){
		System.out.println("Eating");
	}
}
class Dog extends Animal{
	int cookies_num;
	public Dog() {
		super();
		System.out.println(cookies_num);
		System.out.println("Barking");
	}
	{
		cookies_num = 34;
		System.out.println("Hi");
	}
}
public class G2{
	public static void main(String[] args) {
		Dog obj = new Dog();
	}
}
