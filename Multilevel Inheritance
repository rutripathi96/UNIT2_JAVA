//Multilevel Inheritance
class Animal {
	public void eat() {
		System.out.println("Eating");
	}
}
class Dog extends Animal{
	public void bark() {
		System.out.println("Barking");
	}
}
class Pup extends Dog{
	public void cry() {
		System.out.println("Crying");
	}
}
public class Gi2{
	public static void main(String[] args) {
		Pup obj = new Pup();
		obj.eat();
		obj.bark();
		obj.cry();
  }
	
	
}
